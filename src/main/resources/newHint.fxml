<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<VBox id="window" alignment="CENTER_LEFT" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.represc.gui.newCountdown">
   <children>
      <HBox id="header" prefWidth="580.0">
         <children>
            <AnchorPane id="header" prefWidth="556.0" HBox.hgrow="ALWAYS">
               <children>
                  <Label prefHeight="17.0" text="New Countdown" />
               </children>
            </AnchorPane>
            <Button alignment="CENTER_RIGHT" mnemonicParsing="false" text="X" />
         </children>
      </HBox>
      <VBox prefWidth="580.0">
         <children>
            <Label prefHeight="17.0" text="Name" />
            <TextField maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="25.0" prefWidth="300.0" />
         </children>
      </VBox>
      <VBox prefHeight="114.0" prefWidth="580.0">
         <children>
            <Label prefHeight="17.0" text="Description" />
            <TextArea prefWidth="580.0" />
         </children>
      </VBox>
      <VBox>
         <children>
            <Label text="Duration" />
            <HBox spacing="10.0">
               <children>
                  <Spinner prefWidth="60.0" />
                  <Label text="minute(s)" />
                  <Spinner prefWidth="60.0" />
                  <Label text="second(s)" />
               </children>
            </HBox>
         </children>
      </VBox>
      <HBox id="bottom">
         <children>
            <Pane HBox.hgrow="ALWAYS">
               <children>
                  <Button alignment="CENTER" cancelButton="true" layoutX="127.0" mnemonicParsing="false" text="Cancel" textAlignment="CENTER" />
               </children>
            </Pane>
            <Pane HBox.hgrow="ALWAYS">
               <children>
                  <Button alignment="CENTER" layoutX="119.0" mnemonicParsing="false" text="Save" textAlignment="CENTER" />
               </children>
            </Pane>
         </children>
      </HBox>
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</VBox>
